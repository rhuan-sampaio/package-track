        <script><![CDATA[var r = new sn_ws.RESTMessageV2('x_724203_package_t.Get Package ', 'Default GET');
r.setStringParameterNoEscape('track', current.track_code);
var response = r.execute();
var responseBody = response.getBody();
var httpStatus = response.getStatusCode();
var obj = JSON.parse(responseBody);

if(obj.objetos[0].eventos){
var desc = obj.objetos[0].eventos[0].descricao;
var dtHr = obj.objetos[0].eventos[0].dtHrCriado;
var local = obj.objetos[0].eventos[0].unidade.nome;
current.work_notes = 'Status: '+ desc + '\n'+ 'Data/hora: '+dtHr + '\n'+ 'Local: '+ local;
	
action.setRedirectURL(current);
current.update();

}
if(obj.objetos[0].mensagem){
	gs.addErrorMessage(obj.objetos[0].mensagem);
	action.setRedirectURL(current);
	current.update();

}
]]></script>
